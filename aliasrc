# PostGres Mac
alias pgstart="pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start"
alias pgstop="pg_ctl -D /usr/local/var/postgres stop -s -m fast"


#Aliases
alias ll='ls -lGh'
alias ls='ls -Gh'
alias cls='clear'
alias ..="cd ../"
alias ...="cd ../.."
alias ....="cd ../../.."
alias ls='ls --color=auto'
alias la="ls -al"
alias o=$EDITOR
alias dotf="cd $HOME/.dotfiles"
alias t="todo.sh"

